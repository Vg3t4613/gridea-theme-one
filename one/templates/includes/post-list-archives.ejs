<div class="autopagerize_page_element">
  <div class="content">
    <%let years=[];posts.forEach((item)=>{const year=item.date.substring(0,4);if(!years.includes(year)){years.push(year)}});%>
    <%years.forEach(function(year){%>
    <div class="list_with_title animated fadeInDown">
      <div class="listing_title">
        <%-year%></div>
      <div class="listing">
        <div class="listing-item">
          <%posts.forEach(function(post){%>
          <%if(post.date.indexOf(year)!==-1){%>
          <div class="listing_post">
            <a href="<%= post.link %>" title="<%= post.title %>">
              <%=post.title%></a>
            <div class="post_time">
              <span class="date">
                <%=site.utils.moment(post.date).format( "MM.DD")%></span>
            </div>
          </div>
          <%}%>
                    <%});%></div>
      </div>
    </div>
    <%});%></div>
</div>
<%-include( './pagination')%>